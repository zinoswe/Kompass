</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">haversine</span></span><span>(</span><span><span class="hljs-params">lat1, lon1, lat2, lon2</span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">toRad</span></span><span> = (</span><span><span class="hljs-params">x</span></span><span>) =&gt; (x * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-property">PI</span></span><span>) / </span><span><span class="hljs-number">180</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> R = </span><span><span class="hljs-number">6371</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> dLat = </span><span><span class="hljs-title function_">toRad</span></span><span>(lat2 - lat1);
  </span><span><span class="hljs-keyword">const</span></span><span> dLon = </span><span><span class="hljs-title function_">toRad</span></span><span>(lon2 - lon1);
  </span><span><span class="hljs-keyword">const</span></span><span> a =
    </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sin</span></span><span>(dLat / </span><span><span class="hljs-number">2</span></span><span>) ** </span><span><span class="hljs-number">2</span></span><span> +
    </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">cos</span></span><span>(</span><span><span class="hljs-title function_">toRad</span></span><span>(lat1)) * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">cos</span></span><span>(</span><span><span class="hljs-title function_">toRad</span></span><span>(lat2)) * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sin</span></span><span>(dLon / </span><span><span class="hljs-number">2</span></span><span>) ** </span><span><span class="hljs-number">2</span></span><span>;
  </span><span><span class="hljs-keyword">return</span></span><span> R * (</span><span><span class="hljs-number">2</span></span><span> * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">atan2</span></span><span>(</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sqrt</span></span><span>(a), </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sqrt</span></span><span>(</span><span><span class="hljs-number">1</span></span><span> - a)));
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">searchStiftungen</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
