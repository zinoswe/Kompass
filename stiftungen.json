    .filter(
      (s) =&gt;
        s.typ === "regional" &amp;&amp;
        s.lat &amp;&amp; s.lon &amp;&amp;
        s.bundesland.toLowerCase() === bundesland.toLowerCase()
    )
    .map((s) =&gt; ({
      ...s,
      dist: haversine(lat, lon, s.lat, s.lon),
    }))
    .filter((s) =&gt; s.dist &lt;= 50)
    .sort((a, b) =&gt; a.dist - b.dist);

  const bundesweit = stiftungen.filter((s) =&gt; s.typ === "bundesweit");

  if (isRegio &amp;&amp; regionals.length &gt; 0) {
