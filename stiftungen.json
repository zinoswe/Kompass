    </span><span><span class="hljs-keyword">return</span></span><span>;
  }

  </span><span><span class="hljs-keyword">const</span></span><span> plzEntry = </span><span><span class="hljs-title class_">Object</span></span><span>.</span><span><span class="hljs-title function_">entries</span></span><span>(plzDb).</span><span><span class="hljs-title function_">find</span></span><span>(
    </span><span><span class="hljs-function">(<span class="hljs-params">[plz, data]</span></span></span><span>) =&gt;
      plz === input || data.</span><span><span class="hljs-property">ort</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === input
  );

  </span><span><span class="hljs-keyword">if</span></span><span> (!plzEntry) {
    resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">"&lt;p class='text-red-500'&gt;PLZ oder Ort nicht gefunden.&lt;/p&gt;"</span></span><span>;
    </span><span><span class="hljs-keyword">return</span></span><span>;
