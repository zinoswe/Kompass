</span><span><span class="hljs-keyword">let</span></span><span> plzDb = {};
</span><span><span class="hljs-keyword">let</span></span><span> datenGeladen = </span><span><span class="hljs-literal">false</span></span><span>;

</span><span><span class="hljs-comment">// Lade beide JSON-Dateien parallel</span></span><span>
