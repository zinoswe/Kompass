    .</span><span><span class="hljs-title function_">map</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">s</span></span></span><span> =&gt; ({ ...s, </span><span><span class="hljs-attr">dist</span></span><span>: </span><span><span class="hljs-title function_">haversine</span></span><span>(lat, lon, s.</span><span><span class="hljs-property">lat</span></span><span>, s.</span><span><span class="hljs-property">lon</span></span><span>) }))
    .</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">s</span></span></span><span> =&gt; s.</span><span><span class="hljs-property">dist</span></span><span> &lt;= </span><span><span class="hljs-number">50</span></span><span>)
