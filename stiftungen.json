  </span><span><span class="hljs-string">"49074"</span></span><span>: { </span><span><span class="hljs-attr">ort</span></span><span>: </span><span><span class="hljs-string">"Osnabrück"</span></span><span>, </span><span><span class="hljs-attr">bundesland</span></span><span>: </span><span><span class="hljs-string">"Niedersachsen"</span></span><span>, </span><span><span class="hljs-attr">lat</span></span><span>: </span><span><span class="hljs-number">52.2799</span></span><span>, </span><span><span class="hljs-attr">lon</span></span><span>: </span><span><span class="hljs-number">8.0472</span></span><span> },
