    </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">cos</span></span><span>(</span><span><span class="hljs-title function_">toRad</span></span><span>(lat1)) * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">cos</span></span><span>(</span><span><span class="hljs-title function_">toRad</span></span><span>(lat2)) * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sin</span></span><span>(dLon / </span><span><span class="hljs-number">2</span></span><span>) ** </span><span><span class="hljs-number">2</span></span><span>;
  </span><span><span class="hljs-keyword">return</span></span><span> R * (</span><span><span class="hljs-number">2</span></span><span> * </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">atan2</span></span><span>(</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sqrt</span></span><span>(a), </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">sqrt</span></span><span>(</span><span><span class="hljs-number">1</span></span><span> - a)));
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">searchStiftungen</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> input = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"search"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>().</span><span><span class="hljs-title function_">toLowerCase</span></span><span>();
  </span><span><span class="hljs-keyword">const</span></span><span> resultsDiv = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"results"</span></span><span>);
  resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;

  </span><span><span class="hljs-comment">// Eingabe mit PLZ oder Ort vergleichen</span></span><span>
  </span><span><span class="hljs-keyword">const</span></span><span> plzEntry = </span><span><span class="hljs-title class_">Object</span></span><span>.</span><span><span class="hljs-title function_">entries</span></span><span>(plzDb).</span><span><span class="hljs-title function_">find</span></span><span>(
    </span><span><span class="hljs-function">(<span class="hljs-params">[plz, data]</span></span></span><span>) =&gt;
      plz === input || data.</span><span><span class="hljs-property">ort</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === input
  );

  </span><span><span class="hljs-keyword">if</span></span><span> (!plzEntry) {
    resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">"&lt;p class='text-red-500'&gt;PLZ oder Ort nicht gefunden.&lt;/p&gt;"</span></span><span>;
    </span><span><span class="hljs-keyword">return</span></span><span>;
  }

  </span><span><span class="hljs-keyword">const</span></span><span> [plz, ortInfo] = plzEntry;
  </span><span><span class="hljs-keyword">const</span></span><span> isRegio = [</span><span><span class="hljs-string">"Niedersachsen"</span></span><span>, </span><span><span class="hljs-string">"Bayern"</span></span><span>].</span><span><span class="hljs-title function_">includes</span></span><span>(ortInfo.</span><span><span class="hljs-property">bundesland</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> { lat, lon, bundesland } = ortInfo;

  </span><span><span class="hljs-comment">// Regionale Stiftungen (nur wenn in Niedersachsen/Bayern)</span></span><span>
  </span><span><span class="hljs-keyword">const</span></span><span> regionals = stiftungen
    .</span><span><span class="hljs-title function_">filter</span></span><span>(
      </span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt;
        s.</span><span><span class="hljs-property">typ</span></span><span> === </span><span><span class="hljs-string">"regional"</span></span><span> &amp;&amp;
        s.</span><span><span class="hljs-property">lat</span></span><span> &amp;&amp; s.</span><span><span class="hljs-property">lon</span></span><span> &amp;&amp;
        s.</span><span><span class="hljs-property">bundesland</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === bundesland.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>()
    )
    .</span><span><span class="hljs-title function_">map</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt; ({
      ...s,
      </span><span><span class="hljs-attr">dist</span></span><span>: </span><span><span class="hljs-title function_">haversine</span></span><span>(lat, lon, s.</span><span><span class="hljs-property">lat</span></span><span>, s.</span><span><span class="hljs-property">lon</span></span><span>),
    }))
    .</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt; s.</span><span><span class="hljs-property">dist</span></span><span> &lt;= </span><span><span class="hljs-number">50</span></span><span>)
    .</span><span><span class="hljs-title function_">sort</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; a.</span><span><span class="hljs-property">dist</span></span><span> - b.</span><span><span class="hljs-property">dist</span></span><span>);

  </span><span><span class="hljs-keyword">const</span></span><span> bundesweit = stiftungen.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt; s.</span><span><span class="hljs-property">typ</span></span><span> === </span><span><span class="hljs-string">"bundesweit"</span></span><span>);

  </span><span><span class="hljs-keyword">if</span></span><span> (isRegio &amp;&amp; regionals.</span><span><span class="hljs-property">length</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) {
    resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> += <span class="hljs-string">`
      &lt;h2 class="text-xl font-bold text-blue-700 mt-4 mb-2"&gt;üîπ Regionale Stiftungen in Ihrer N√§he&lt;/h2&gt;`</span>;
    regionals.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt; {
      resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> += </span><span><span class="hljs-title function_">renderStiftung</span></span><span>(s, </span><span><span class="hljs-literal">true</span></span><span>);
    });
  }

  </span><span><span class="hljs-keyword">if</span></span><span> (bundesweit.</span><span><span class="hljs-property">length</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) {
    resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> += <span class="hljs-string">`
      &lt;h2 class="text-xl font-bold text-gray-700 mt-8 mb-2"&gt;üåç Bundesweite Stiftungen&lt;/h2&gt;`</span>;
    bundesweit.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">s</span></span></span><span>) =&gt; {
      resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> += </span><span><span class="hljs-title function_">renderStiftung</span></span><span>(s, </span><span><span class="hljs-literal">false</span></span><span>);
    });
  }

  </span><span><span class="hljs-keyword">if</span></span><span> (!isRegio &amp;&amp; !bundesweit.</span><span><span class="hljs-property">length</span></span><span>) {
    resultsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">"&lt;p class='text-gray-500'&gt;Keine Stiftungen gefunden.&lt;/p&gt;"</span></span><span>;
  }
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">renderStiftung</span></span><span>(</span><span><span class="hljs-params">s, showDist</span></span><span>) {
  </span><span><span class="hljs-keyword">return</span></span><span> <span class="hljs-string">`
    &lt;div class="bg-blue-50 border border-blue-200 p-5 rounded-lg shadow-sm"&gt;
      &lt;h2 class="text-xl font-bold text-blue-800"&gt;</span></span><span><span class="hljs-subst">${s.name}</span></span><span>&lt;/h2&gt;
      &lt;p class="text-sm text-gray-700"&gt;
        üìç Bundesland: </span><span><span class="hljs-subst">${s.bundesland}</span></span><span><span class="hljs-subst">${showDist &amp;&amp; s.dist ? <span class="hljs-string">` ‚Äì <span class="hljs-subst">${s.dist.toFixed(<span class="hljs-number">1</span></span></span></span></span><span>)}‚ÄØkm entfernt` : </span><span><span class="hljs-string">""</span></span><span>}
      &lt;/p&gt;
      &lt;p class="mt-2 text-gray-800 leading-relaxed"&gt;
        </span><span><span class="hljs-subst">${s.beschreibung.slice(<span class="hljs-number">0</span></span></span><span>, </span><span><span class="hljs-number">400</span></span><span>)}...
      &lt;/p&gt;
      <span class="hljs-subst">${
        s.link
          ? </span></span><span><span class="hljs-string">`&lt;a href="<span class="hljs-subst">${s.link}</span></span></span><span>" target="_blank" class="inline-block mt-3 text-blue-600 hover:underline"&gt;üîó Zum Antrag / Infos&lt;/a&gt;`
          : </span><span><span class="hljs-string">""</span></span><span>
      }
    &lt;/div&gt;
  `;
}
