      &lt;p class="mt-2 text-gray-800 leading-relaxed"&gt;
        </span><span><span class="hljs-subst">${s.beschreibung.slice(<span class="hljs-number">0</span></span></span><span>, </span><span><span class="hljs-number">400</span></span><span>)}...
      &lt;/p&gt;
      </span><span><span class="hljs-subst">${s.link ? <span class="hljs-string">`&lt;a href="<span class="hljs-subst">${s.link}</span></span></span></span><span>" target="_blank" class="inline-block mt-3 text-blue-600 hover:underline"&gt;üîó Zum Antrag / Infos&lt;/a&gt;` : </span><span><span class="hljs-string">""</span></span><span>}
    &lt;/div&gt;
  `;
}
