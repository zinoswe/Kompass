let plzDb = {};
let datenGeladen = false;

Promise.all([
  fetch("stiftungen.json").then((res) =&gt; res.json()),
  fetch("plz-db.json").then((res) =&gt; res.json())
]).then(([stiftungenData, plzData]) =&gt; {
  stiftungen = stiftungenData;
  plzDb = plzData;
  datenGeladen = true;
});

function haversine(lat1, lon1, lat2, lon2) {
  const toRad = (x) =&gt; (x * Math.PI) / 180;
  const R = 6371;
  const dLat = toRad(lat2 - lat1);
  const dLon = toRad(lon2 - lon1);
  const a =
    Math.sin(dLat / 2) ** 2 +
    Math.cos(toRad(lat1)) * Math.cos(toRad(lat2)) * Math.sin(dLon / 2) ** 2;
  return R * (2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a)));
}

function searchStiftungen() {
  const input = document.getElementById("search").value.trim().toLowerCase();
  const resultsDiv = document.getElementById("results");
  resultsDiv.innerHTML = "";

  if (!datenGeladen) {
    resultsDiv.innerHTML = "&lt;p class='text-yellow-500'&gt;⏳ Die Daten werden noch geladen. Bitte warten...&lt;/p&gt;";
    return;
  }

  const plzEntry = Object.entries(plzDb).find(
    ([plz, data]) =&gt;
      plz === input || data.ort.toLowerCase() === input
  );

  if (!plzEntry) {
    resultsDiv.innerHTML = "&lt;p class='text-red-500'&gt;PLZ oder Ort nicht gefunden.&lt;/p&gt;";
    return;
  }

  const [plz, ortInfo] = plzEntry;
  const isRegio = ["Niedersachsen", "Bayern"].includes(ortInfo.bundesland);
  const { lat, lon, bundesland } = ortInfo;

  const regionals = stiftungen
    .filter(
      (s) =&gt;
        s.typ === "regional" &amp;&amp;
        s.lat &amp;&amp; s.lon &amp;&amp;
        s.bundesland.toLowerCase() === bundesland.toLowerCase()
    )
    .map((s) =&gt; ({
      ...s,
      dist: haversine(lat, lon, s.lat, s.lon),
    }))
    .filter((s) =&gt; s.dist &lt;= 50)
    .sort((a, b) =&gt; a.dist - b.dist);

  const bundesweit = stiftungen.filter((s) =&gt; s.typ === "bundesweit");

  if (isRegio &amp;&amp; regionals.length &gt; 0) {
    resultsDiv.innerHTML += <code>
</code>      &lt;h2 class="text-xl font-bold text-blue-700 mt-4 mb-2"&gt;🔹 Regionale Stiftungen in Ihrer Nähe&lt;/h2&gt;<code>;
</code>    regionals.forEach((s) =&gt; {
      resultsDiv.innerHTML += renderStiftung(s, true);
    });
  }

  if (bundesweit.length &gt; 0) {
    resultsDiv.innerHTML += <code>
</code>      &lt;h2 class="text-xl font-bold text-gray-700 mt-8 mb-2"&gt;🌍 Bundesweite Stiftungen&lt;/h2&gt;<code>;
</code>    bundesweit.forEach((s) =&gt; {
      resultsDiv.innerHTML += renderStiftung(s, false);
    });
  }

  if (!isRegio &amp;&amp; bundesweit.length === 0) {
    resultsDiv.innerHTML = "&lt;p class='text-gray-500'&gt;Keine Stiftungen gefunden.&lt;/p&gt;";
  }
}

